<b><p class="lead mb-3" style="text-align: center;">Welcome <%= name %></p></b>

<br>

<% for(let i = 0; i < notes.length; i++) { %>
<div class="card text-white bg-primary mb-3" style="max-width: 18rem;">
    <div class="card-header"><%= notes[i].title %></div>
    <div class="card-body">
        <p class="card-text"><%= notes[i].note %></p>
    </div>
</div>
<% } %>

<br>

<a href="/note/createNote" class="btn btn-primary btn-lg" role="button" aria-pressed="true"><h3>+</h3></a>